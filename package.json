{
  "name": "react-router-starter",
  "description": "Build a full-stack web application with React Router 7.",
  "private": true,
  "cloudflare": {
    "label": "React Router Framework Starter",
    "products": [
      "Workers"
    ],
    "categories": [
      "storage"
    ],
    "preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/36a99c4a-8919-4e6e-ac9e-e55974407200/public",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/65455e96-7992-425f-44e4-30f3e7d7f900/preview",
    "publish": true
  },
  "dependencies": {
    "@jsquash/jpeg": "^1.6.0",
    "@jsquash/png": "^3.1.1",
    "@jsquash/resize": "^2.1.0",
    "@jsquash/webp": "^1.5.0",
    "isbot": "5.1.26",
    "lucide-react": "^0.554.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "7.6.3",
    "recharts": "^3.4.1",
    "three": "^0.181.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.7.5",
    "@cloudflare/workers-types": "^4.20251120.0",
    "@react-router/dev": "7.6.3",
    "@tailwindcss/vite": "4.1.4",
    "@types/node": "24.0.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.2.1",
    "execa": "^9.6.0",
    "express": "^5.1.0",
    "playwright": "^1.50.0",
    "simple-git": "^3.30.0",
    "tailwindcss": "4.1.1",
    "tsx": "^4.20.6",
    "typescript": "5.8.3",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.49.0"
  },
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types && react-router typegen",
    "check": "tsc && react-router build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "react-router dev",
    "dev:remote:ui": "concurrently --kill-others-on-fail -n APP,TB \"npm run dev\" \"npm:toolbelt\"",
    "dev:local:admin": "npm run build && concurrently --kill-others-on-fail -n WRANGLER,TB \"wrangler dev\" \"npm:toolbelt\"",
    "dev:remote:admin": "npm run build && concurrently --kill-others-on-fail -n WRANGLER,TB \"node scripts/run-wrangler-remote.mjs\" \"npm:toolbelt\"",
    "wrangler:login": "node scripts/run-wrangler-login.mjs",
    "sync:d1:remote-to-local": "node scripts/sync-d1-remote-to-local.mjs",
    "sync:d1:local-to-remote": "node scripts/sync-d1-local-to-remote.mjs",
    "sync:blog-json-to-d1:local": "npx tsx scripts/migrate-blog-json-to-d1.mts && npx wrangler d1 execute blog-db --local --file=blog-migrate.sql",
    "sync:blog-json-to-d1:remote": "npx tsx scripts/migrate-blog-json-to-d1.mts && npx wrangler d1 execute blog-db --remote --file=blog-migrate.sql",
    "spotify:import": "node scripts/spotify-import.mjs --remote",
    "spotify:import:local": "node scripts/spotify-import.mjs --local",
    "spotify:token": "node scripts/spotify-token.mjs",
    "preview": "npm run build && vite preview",
    "typecheck": "npm run typegen && tsc -b",
    "dump:routes": "pwsh ./dump-routes.ps1",
    "toolbelt": "node scripts/run-toolbelt.mjs",
    "dev:ui": "concurrently --kill-others-on-fail -n APP,TB \"npm:dev\" \"npm:toolbelt\""
  },
  "type": "module"
}

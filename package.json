{
  "name": "react-router-starter",
  "description": "Build a full-stack web application with React Router 7.",
  "private": true,
  "cloudflare": {
    "label": "React Router Framework Starter",
    "products": [
      "Workers"
    ],
    "categories": [
      "storage"
    ],
    "preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/36a99c4a-8919-4e6e-ac9e-e55974407200/public",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/65455e96-7992-425f-44e4-30f3e7d7f900/preview",
    "publish": true
  },
  "dependencies": {
    "isbot": "5.1.26",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "7.6.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.7.5",
    "@cloudflare/workers-types": "^4.20251120.0",
    "@react-router/dev": "7.6.3",
    "@tailwindcss/vite": "4.1.4",
    "@types/node": "24.0.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.2.1",
    "execa": "^9.6.0",
    "express": "^5.1.0",
    "simple-git": "^3.30.0",
    "tailwindcss": "4.1.1",
    "tsx": "^4.20.6",
    "typescript": "5.8.3",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.49.0"
  },
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types && react-router typegen",
    "check": "tsc && react-router build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "react-router dev",
    "dev:remote:ui": "concurrently -k -n APP,TB \"npm run dev\" \"npm:toolbelt\"",
    "dev:local:admin": "npm run build && concurrently -k -n WRANGLER,TB \"wrangler dev\" \"npm:toolbelt\"",
    "dev:remote:admin": "npm run build && concurrently -k -n WRANGLER,TB \"wrangler dev --remote\" \"npm:toolbelt\"",
    "sync:d1:remote-to-local": "node -e \"require('fs').mkdirSync('dumps',{recursive:true})\" && npx wrangler d1 execute blog-db --remote --command \".dump\" > dumps/d1-remote.sql && npx wrangler d1 execute blog-db --local --file=dumps/d1-remote.sql",
    "preview": "npm run build && vite preview",
    "typecheck": "npm run typegen && tsc -b",
    "dump:routes": "pwsh ./dump-routes.ps1",
    "toolbelt": "tsx toolbelt/index.ts",
    "dev:ui": "concurrently -k -n APP,TB \"npm:dev\" \"npm:toolbelt\""
  },
  "type": "module"
}
